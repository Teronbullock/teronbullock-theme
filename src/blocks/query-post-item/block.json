{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "tb-theme/query-post-item",
  "version": "0.0.2",
  "title": "TB: Query post-item",
  "category": "theme",
  "icon": "index-card",
  "description": "Displaying a card that has a link to a post or page.",
  "keywords": ["TB Theme"],
  "example": {},
  "parent": ["tb-theme/tb-query"],
  "attributes": {
    "align": {
      "type": "string",
      "default": "full"
    },
    "postLinkType": {
      "type": "string",
      "default": "post"
    }
  },
  "supports": {
    "html": false,
    "align": true
  },
  "textdomain": "tb-theme",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php"
}
