{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 3,
  "settings": {
    "appearanceTools": true,
    "color": {
      "custom": true,
      "defaultPalette": false,
      "palette": [
        {
          "name": "Primary",
          "slug": "primary",
          "color": "#0f3952"
        },
        {
          "name": "Secondary",
          "slug": "secondary",
          "color": "#102D12"
        },
        {
          "name": "Tertiary",
          "slug": "tertiary",
          "color": "#B6B9B9"
        },
        {
          "name": "Jasmine",
          "slug": "jasmine",
          "color": "#FFD275"
        },
        {
          "name": "Red",
          "slug": "red",
          "color": "#d74058"
        },
        {
          "name": "White",
          "slug": "white",
          "color": "#ffffff"
        },
        {
          "name": "Black",
          "slug": "black",
          "color": "#000000"
        },
        {
          "name": "White Smoke",
          "slug": "white-smoke",
          "color": "#F5F5F5"
        },
        {
          "name": "Black Night",
          "slug": "black",
          "color": "#141515"
        }
      ]
    },
    "layout": {
      "contentSize": "1000px",
      "wideSize": "1440px"
    },
    "typography": {
      "customFontSize": false,
      "fluid": true,
      "defaultFontSizes": false,
      "fontSizes": [
        {
          "name": "Extra Small",
          "slug": "xs",
          "size": "1.125rem",
          "fluid": {
            "min": "0.9375rem",
            "max": "1.125rem"
          }
        },
        {
          "name": "Small",
          "slug": "sm",
          "size": "1.25rem",
          "fluid": {
            "min": "1rem",
            "max": "1.25rem"
          }
        },
        {
          "name": "Medium",
          "slug": "md",
          "size": "1.5rem",
          "fluid": {
            "min": "1.125rem",
            "max": "1.5rem"
          }
        },
        {
          "name": "Large",
          "slug": "lg",
          "size": "1.625rem",
          "fluid": {
            "min": "1.25rem",
            "max": "1.625rem"
          }
        },
        {
          "name": "X Large",
          "slug": "xl",
          "size": "2rem",
          "fluid": {
            "min": "1.375rem",
            "max": "2rem"
          }
        },
        {
          "name": "XX Large",
          "slug": "xxl",
          "size": "2.5rem",
          "fluid": {
            "min": "1.75rem",
            "max": "2.5rem"
          }
        }
      ],
      "fontFamilies": [
        {
          "fontFamily": "Poppins, Helvetica, Arial, Lucida, sans-serif",
          "name": "Poppins",
          "slug": "main",
          "fontFace": [
            {
              "fontFamily": "Poppins",
              "fontStyle": "regular",
              "fontStretch": "normal",
              "src": [
                "file:./assets/fonts/poppins-v20-latin/poppins-v20-latin-regular.woff2"
              ]
            },
            {
              "fontFamily": "Poppins",
              "fontStyle": "italic",
              "fontStretch": "normal",
              "src": [
                "file:./assets/fonts/poppins-v20-latin/poppins-v20-latin-italic.woff2"
              ]
            },
            {
              "fontFamily": "Poppins",
              "fontWeight": "500",
              "fontStyle": "normal",
              "fontStretch": "normal",
              "src": [
                "file:./assets/fonts/poppins-v20-latin/poppins-v20-latin-500.woff2"
              ]
            },
            {
              "fontFamily": "Poppins",
              "fontWeight": "600",
              "fontStyle": "normal",
              "fontStretch": "normal",
              "src": [
                "file:./assets/fonts/poppins-v20-latin/poppins-v20-latin-600.woff2"
              ]
            }
          ]
        },
        {
          "fontFamily": "Lora, sans-serif",
          "name": "Lora",
          "slug": "body",
          "fontFace": [
            {
              "fontFamily": "Lora",
              "fontStyle": "regular",
              "fontStretch": "normal",
              "src": [
                "file:./assets/fonts/lora-v32-latin/lora-v32-latin-regular.woff2"
              ]
            },
            {
              "fontFamily": "Lora",
              "fontStyle": "italic",
              "fontStretch": "normal",
              "src": [
                "file:./assets/fonts/lora-v32-latin/lora-v32-latin-italic.woff2"
              ]
            },
            {
              "fontFamily": "Lora",
              "fontWeight": "600",
              "fontStyle": "normal",
              "fontStretch": "normal",
              "src": [
                "file:./assets/fonts/lora-v32-latin/lora-v32-latin-600.woff2"
              ]
            }
          ]
        }
      ]
    },
    "blocks": {
      "core/group": {
        "layout": {
          "contentSize": "1000px",
          "wideSize": "1440px"
        }
      }
    }
  },
  "styles": {
    "elements": {
      "h1": {
        "typography": {
          "fontWeight": "600",
          "fontSize": "var(--wp--preset--font-size--xxl)"
        }
      },
      "h2": {
        "typography": {
          "fontWeight": "500",
          "fontSize": "var(--wp--preset--font-size--xl)"
        }
      },
      "heading": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--main)"
        }
      }
    },
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--main)"
    },
    "blocks": {
      "core/site-title": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--main)"
        }
      },
      "core/navigation": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--main)"
        }
      },
      "core/paragraph": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--main)",
          "fontSize": "var(--wp--preset--font-size--sm)",
          "lineHeight": "1.5"
        },
        "spacing": {
          "margin": {
            "bottom": "var:preset|spacing|50"
          }
        }
      },
      "tb-theme/category-display-block": {
        "color": {
          "background": "var(--wp--preset--color--primary)",
          "text": "var(--wp--preset--color--white)"
        },
        "spacing": {
          "padding": {
            "top": "0.5rem",
            "right": "0.5rem",
            "bottom": "0.5rem",
            "left": "0.5rem"
          },
          "margin": {
            "right": "var(--wp-preset--spacing--80)",
            "bottom": "0"
          }
        }
      }
    }
  },
  "templateParts": [
    {
      "area": "header",
      "name": "header",
      "title": "Header"
    },
    {
      "area": "footer",
      "name": "footer",
      "title": "Footer"
    }
  ]
}
